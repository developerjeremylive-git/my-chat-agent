{
  "name": "my-chat-agent",
  "main": "src/server.ts",
  "compatibility_date": "2025-02-04",
  "compatibility_flags": [
    "nodejs_compat",
    "nodejs_compat_populate_process_env"
  ],
  "assets": {
    "directory": "public"
  },
  "durable_objects": {
    "bindings": [
      {
        "name": "Chat",
        "class_name": "Chat"
      }
    ]
  },
  "migrations": [
    {
      "tag": "v1",
      "new_sqlite_classes": [
        "Chat"
      ]
    }
  ],
  "ai": {
    "binding": "AI"
  },
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "database",
      "database_id": "bccc0a52-01a9-44c8-87ed-6cd658842536"
    }
  ],
  "kv_namespaces": [
    {
      "binding": "MODEL_CONFIG",
      "id": "8a6552293bb845b38cdcfb687c7fe846",
      "preview_id": ""  // Optional: For preview environments
    }
  ]
}